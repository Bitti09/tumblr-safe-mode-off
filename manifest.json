{
  "name": "Tumblr | Safe Mode Off",
  "description": "Skip all \"safe mode\" warnings and display blogs fullscreen",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": { "scripts":["background.js"] },
  "icons": {
    "16": "img/icon/icon-16.png",
    "48": "img/icon/icon-48.png"
  },
  "content_scripts": [{
    "matches": ["https://www.tumblr.com/dashboard/blog/*"],
    "css": [ "blog.css"],
    "js": ["blog.js"]
  }],
  "permissions": [
    "*://*.tumblr.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "dashboard.js"
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.bootstrapcdn.com; object-src 'self'"
}
